{
 "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
 "vcs": {
  "clientKind": "git"
 },
 "files": {
  "ignoreUnknown": true
 },
 "formatter": {
  "enabled": true,
  "formatWithErrors": true,
  "indentStyle": "space",
  "indentWidth": 1,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "ignore": ["src/asset"]
 },
 "organizeImports": {
  "enabled": true,
  "include": ["src/**/*.ts", "src/**/*.tsx"]
 },
 "linter": {
  "enabled": true,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "ignore": ["src/asset"],
  "rules": {
   "recommended": true,
   "suspicious": {
    "noShadowRestrictedNames": "off"
   }
  }
 },
 "javascript": {
  "formatter": {
   "quoteStyle": "single",
   "arrowParentheses": "asNeeded"
  }
 }
}
